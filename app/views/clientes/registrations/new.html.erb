<h2>Sign up</h2>

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= render "clientes/shared/error_messages", resource: resource %>

  <div class="field">
    <%= f.label :nome %><br />
    <%= f.text_field :nome, autofocus: true, autocomplete: "nome" %>
  </div>

  <div class="field">
    <%= f.label :cpf %><br />
    <%= f.text_field :cpf, autofocus: true, autocomplete: "cpf" %>
  </div>

  <div class="field">
    <%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
  </div>

  <div class="field">
    <%= f.label :cep %><br />
    <%= f.text_field :cep, autofocus: true, autocomplete: "CEP" %>
  </div>

  <div class="field">
    <%= f.label :cidade %><br />
    <%= f.text_field :cidade, autofocus: true, autocomplete: "cidade" %>
  </div>

  <div class="field">
    <%= f.label :bairro %><br />
    <%= f.text_field :bairro, autofocus: true, autocomplete: "bairro" %>
  </div>

  <div class="field">
    <%= f.label :logradouro %><br />
    <%= f.text_field :logradouro, autofocus: true, autocomplete: "logradouro" %>
  </div>

  <div class="field">
    <%= f.label :complemento %><br />
    <%= f.text_field :complemento, autofocus: true, autocomplete: "complemento" %>
  </div>

  <div class="field">
    <%= f.label :password %>
    <% if @minimum_password_length %>
    <em>(<%= @minimum_password_length %> characters minimum)</em>
    <% end %><br />
    <%= f.password_field :password, autocomplete: "new-password" %>
  </div>

  <div class="field">
    <%= f.label :password_confirmation %><br />
    <%= f.password_field :password_confirmation, autocomplete: "new-password" %>
  </div>

  <div class="actions">
    <%= f.submit "Sign up" %>
  </div>
<% end %>

<%= render "clientes/shared/links" %>
